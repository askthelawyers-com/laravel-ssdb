{
    "name": "huangdijia/laravel-ssdb",
    "description": "ssdb for laravel",
    "type": "library",
    "keywords": [
        "laravel",
        "ssdb"
    ],
    "homepage": "https://github.com/huangdijia/laravel-ssdb",
    "license": "MIT",
    "authors": [
        {
            "name": "huangdijia",
            "email": "huangdijia@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "illuminate/support": "^5.7|^5.8|^6.0|^7.0",
        "illuminate/contracts": "^5.7|^5.8|^6.0|^7.0"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Huangdijia\\Ssdb\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Huangdijia\\Ssdb\\SsdbServiceProvider"
            ],
            "aliases": {
                "Ssdb": "Huangdijia\\Ssdb\\Facades\\Ssdb"
            }
        }
    }
}